{
  "name": "listful",
  "description": "listfulは、フロントエンドエンジニアのためのチートシート型技術ブログです。",
  "version": "0.2.0",
  "scripts": {
    "dev": "next -p 8853",
    "build": "next build",
    "start": "next start -p 8853",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format-watch": "npm run format && onchange \"**/*.{js,jsx,json,md}\" -- prettier --write {{changed}}",
    "ogp": "node scripts/svg-to-png.js",
    "deploy": "git push origin main && vercel --prod"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@mdx-js/loader": "^1.6.16",
    "@mdx-js/react": "^1.6.18",
    "@mdx-js/runtime": "^1.6.18",
    "@next/mdx": "^9.5.1",
    "gray-matter": "^4.0.2",
    "next": "^9.5.1",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "sass": "^1.26.11"
  },
  "devDependencies": {
    "next-images": "^1.5.0",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "sharp": "^0.26.1"
  },
  "repository": "https://github.com/kurachiweb/listful",
  "license": "MIT"
}
